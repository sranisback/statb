<tr>
    <td><span class="glyphicon glyphicon-remove text-danger" playerId="{{player.playerId}}" id="remove_pl_{{player.playerId}}"></span></td>
    <td>{{player.nr}}</td>
    <td>{{player.name}}</td>
    <td><img src="{{ asset('build/'~app.environment~'/images/player_icons/'~player.icon.iconName~'.gif') }}" class=".img-responsive">{{position.pos}}</td>
    <td>{{position.ma}}</td>
    <td>{{position.st}}</td>
    {% if (player.ruleset == 0) %}
        <td>{{position.ag}}</td>
        <td>{{position.av}}</td>
    {% else %}
        <td>{{position.ag}}+</td>
        <td>{{position.cp}}+</td>
        <td>{{position.av}}+</td>
    {% endif %}
    <td>{{skill|raw}}</td>
    <td>0</td>
    <td></td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td>0</td>
    <td class="text-right">{{cout}}</td>
</tr>