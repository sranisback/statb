<table class="table table-striped" id="tableConfrontations">
    <thead>
        <tr>
            <th>adversaire</th>
            <th>% victoire</th>
            <th>Matches</th>
        </tr>
    </thead>
    <tbody>
    {% for coach, line in  tableauConfrontation %}
        {% if line[0] != 'N/A' %}
        <tr data-toggle="tooltip" data-placement="top" title="V: {{ line[1] }}, N: {{ line[2] }}, D: {{ line[3] }} ">
            <td>
                {{ coach }}
            </td>
            <td >
                {{ line[0] }}
            </td>
            <td>
               <a href="{{ path('matchesContreCoach', {'coachId': line[4]}) }}">Voir</a>
            </td>
        </tr>
        {% endif %}
    {% endfor  %}
    </tbody>
</table>