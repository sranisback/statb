{% extends 'statbb/base.html.twig' %}


{% block body %}
<div class = "card m-2">
    <div class="card-header">
        <h1>Voulez vous dire : </h1>
    </div>
    <div class="card-body">
    {% for equipe in listeEquipe %}
        <a href="{{ path('team', {'teamId': equipe.teamId }) }}">{{ equipe.name }}</a>, les {{ equipe.fRace == null ? equipe.race.name : equipe.fRace.name }} de {{ equipe.ownedByCoach.username }} ({{ annees[equipe.year] }}) ? <br/>
    {% endfor %}
    </div>
</div>
{% endblock %}