	<div class="row">
	  <div class="form-group">
		<div class="col-sm-9 col-lg-9 col-md-9">
		<label>Joueur</label>
			<select class="form-control" id="selected_player_{{teamId}}_{{nbr}}" name= "player[{{nbr}}].id">
					<option selected></option>
				{% for player in players %}
					<option value="{{player.playerId}}">{{player.nr}}.{{player.name}}({{player.fRid.name}}, {{player.fPos.pos}})</option>
				{% endfor %}
			</select>
		</div>
		<div class="col-sm-3 col-lg-3 col-md-3">
			<label>Action</label>
			<select class="form-control" id="action" name="player[{{nbr}}].action">
