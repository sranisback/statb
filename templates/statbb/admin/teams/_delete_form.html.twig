<form method="post" action="{{ path('teams_delete', {'teamId': team.teamId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ team.teamId) }}">
    <button class="btn">Delete</button>
</form>
